{
    "presets": [
      ["@babel/preset-env", { "modules": "cjs" }],
      ["@babel/preset-typescript", { "allowDeclareFields": true }]
    ],
    "plugins": [
        ["module-resolver", {
            "root": ["./src"],
            "extensions": [".js", ".ts", ".cjs"],
            "alias": {
              "@": "./src"
            }
          }]
    ]
  }